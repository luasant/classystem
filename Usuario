//classe não executável de usuários

public class usuarios
    {
      private String nome;
      private int matricula;
      
      public void setNome (String n)
      {
          nome = n;
      }
      
      public String getNome()
      {
          return nome;
      }
      
      public void setMatricula (int m)
      {
          matricula = m;
      }
      
      public int getMatricula()
      {
          return matricula;
      }
    }
